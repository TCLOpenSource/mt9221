# USB NAND PATCH
#===================================================
config MS_NAND_MIU1_PATCH
    bool
    default n

config MSTAR_COOCAA_AUTH
    bool "Link with COOCAA Authorization"
    default n

#===================================================
# CPU HOTPLUG
#===================================================
config MSTAR_CPU_HOTPLUG
    bool "Enable Mstar cpu hotplug"
    depends on HOTPLUG_CPU
    default n


