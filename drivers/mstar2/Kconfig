config MSTAR_3RD_PARTY
bool "3rd party device list"
if MSTAR_3RD_PARTY
    config MSTAR_TUNER
    tristate "TUNER"
    help
        MStar TUNER driver function

    menu "Tuner device support list"
    depends on MSTAR_TUNER

        config MSTAR_TUNER_Si2151
            bool "MSTAR_TUNER_Si2151"
        config MSTAR_TUNER_MXL661
            bool "MSTAR_TUNER_MXL661"
        config MSTAR_TUNER_AV2018
            bool "MSTAR_TUNER_AV2018"
        config MSTAR_TUNER_AV2017
            bool "MSTAR_TUNER_AV2017"
        config MSTAR_TUNER_AV2012
            bool "MSTAR_TUNER_AV2012"
        config MSTAR_TUNER_R842
            bool "MSTAR_TUNER_R842"
         config MSTAR_TUNER_RT710
            bool "MSTAR_TUNER_RT710"
    endmenu

    config MSTAR_DISH
    tristate "DISH"
    help
        MStar DISH driver function

    menu "DISH device support list"
    depends on MSTAR_DISH

        config MSTAR_DISH_RT5047
            bool "MSTAR_DISH_RT5047"
    endmenu


endif

