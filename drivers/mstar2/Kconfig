#===================================================
# MStar Chip (auto-select)
# 0: None
# 1: Titania
# 2: Triton
# 3: Titania2
#===================================================
config MSTAR_CHIP_TYPE
	int
	default 1 if MSTAR_TITANIA
	default 2 if MSTAR_TRITON
	default 3 if MSTAR_TITANIA2
	default 4 if MSTAR_OBERON
	default 5 if MSTAR_EUCLID
	default 0

#===================================================
# MStar Chip Name (auto-select)
#===================================================
config MSTAR_CHIP_NAME
	string
	default "einstein" if MSTAR_EINSTEIN
	default "napoli" if MSTAR_NAPOLI
	default "madison" if MSTAR_MADISON
	default "messi" if MSTAR_MESSI
	default "mustang" if MSTAR_MUSTANG
	default "miami" if MSTAR_MIAMI
	default "clippers" if MSTAR_CLIPPERS
	default "monaco" if MSTAR_MONACO
	default "maldives" if MSTAR_MALDIVES
	default "kaiser" if MSTAR_KAISER
	default "muji" if MSTAR_MUJI
	default "muji" if MSTAR_MUJI_32
	default "kano" if MSTAR_KANO
	default "kano" if MSTAR_KANO_32
	default "curry" if MSTAR_CURRY
	default "curry" if MSTAR_CURRY_32
	default "c2p" if MSTAR_C2P
	default "c2p" if MSTAR_C2P_32
	default "monet" if MSTAR_MONET
	default "monet" if MSTAR_MONET_32
	default "manhattan" if MSTAR_MANHATTAN
	default "maserati" if MSTAR_MASERATI
	default "maserati" if MSTAR_MASERATI_32
	default "maxim" if MSTAR_MAXIM
	default "maxim" if MSTAR_MAXIM_32
	default "m7621" if MSTAR_M7621
    default "m7622" if MSTAR_M7622
	default "m7821" if MSTAR_M7821
	default "m5621" if MSTAR_M5621
	default "mainz" if MSTAR_MAINZ
	default "mainz" if MSTAR_MAINZ_32
	default "k6" if MSTAR_K6
	default "k6" if MSTAR_K6_32
        default "k6lite" if MSTAR_K6Lite
	default "k7u" if MSTAR_K7U
	default "k7u" if MSTAR_K7U_32
	default "kris" if MSTAR_KRIS
	default "mooney" if MSTAR_MOONEY
	default "m7221" if MSTAR_M7221
	default "m7322" if MSTAR_M7322
	default "m3822" if MSTAR_M3822
	default "m7632" if MSTAR_M7632
	default "m7332" if MSTAR_M7332
	default "m7642" if MSTAR_M7642
	default "mt5862" if MSTAR_MT5862
	default "mt5867" if MSTAR_MT5867
	default "mt5889" if MSTAR_MT5889
	default "mt5872" if MSTAR_MT5872
	default "error"

