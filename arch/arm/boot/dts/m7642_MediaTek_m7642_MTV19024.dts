                        resume {
                                stage0 {
                                        post-condition = "pnl";
                                };
                        };
                };

                ace {
                        suspend {
                                stage0 {
                                        post-condition = "ace";
                                };
                        };
                        resume {
                                stage0 {
                                        post-condition = "ace";
                                        pre-condition = "xc";
                                };
                        };
                };

                ve {
                        resume {
                                stage0 {
