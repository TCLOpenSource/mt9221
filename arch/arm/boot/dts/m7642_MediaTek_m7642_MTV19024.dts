                                };
                        };
                };

                xc {
                        suspend {
                                stage0 {
                                        pre-condition = "pq","ace";
                                        post-condition = "xc";
                                };
                        };
                        resume {
                                stage0 {
                                        pre-condition = "vdec","avd","miu","mvop","pnl";
