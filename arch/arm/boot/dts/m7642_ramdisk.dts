					pre-condition = "hdmi_rx";
				};
			};
		};

		hdmi_rx {

			resume {

				stage0 {
                                        pre-condition = "hdmi_earc";
					post-condition = "hdmi_rx";
				};
			};
		};

		hdmi_earc {

			resume {

				stage0 {
					post-condition = "hdmi_earc";
				};
			};
		};

		avd {
			resume {

				stage0 {
					pre-condition = "vif";
				};
			};
